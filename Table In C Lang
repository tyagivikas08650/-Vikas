#include <stdio.h>
#include <conio.h>

int main()
{
int Table;
printf("Enter Your Number: ");
scanf("%d", &Table);

   if( Table >=1 && Table <=99)
   {
    for (int i = 1; i <=10; i++)
    printf ("%d \n",i*Table );  
    
   }
   else
   {
    printf("Dont Use Invalid char and num");
   }  
    return 0;
}
